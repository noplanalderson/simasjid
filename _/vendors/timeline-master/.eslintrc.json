{
    "extends": "airbnb-base",
    "env": {
        "browser": true,
        "es6": true
    },
    "rules": {
        "comma-dangle": 0,
        "consistent-return": 0,
        "func-names": 0,
        "linebreak-style": 0,
        "max-len": 0,
        "no-console": 0,
        "no-param-reassign": [
            2,
            {
                "props": false
            }
        ],
        "space-before-function-paren": 0
    }
}